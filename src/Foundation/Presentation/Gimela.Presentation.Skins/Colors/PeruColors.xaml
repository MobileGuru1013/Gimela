<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PeruRankColor1">#FFE2AA8B</Color>
    <Color x:Key="PeruRankColor2">#FFD89F7C</Color>
    <Color x:Key="PeruRankColor3">#FFCB946B</Color>
    <Color x:Key="PeruRankColor4">#FFB48058</Color>
    <Color x:Key="PeruRankColor5">#FFAA7955</Color>
    <Color x:Key="PeruRankColor6">#FF96683F</Color>
    <Color x:Key="PeruRankColor7">#FF7D5331</Color>

    <LinearGradientBrush x:Key="PeruRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource PeruRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource PeruRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource PeruRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource PeruRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource PeruRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource PeruRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource PeruRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>