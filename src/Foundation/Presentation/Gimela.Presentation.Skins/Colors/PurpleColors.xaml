<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PurpleRankColor1">#FFCFADFF</Color>
    <Color x:Key="PurpleRankColor2">#FFA680DE</Color>
    <Color x:Key="PurpleRankColor3">#FF9067CE</Color>
    <Color x:Key="PurpleRankColor4">#FF754CB4</Color>
    <Color x:Key="PurpleRankColor5">#FF593193</Color>
    <Color x:Key="PurpleRankColor6">#FF431C79</Color>
    <Color x:Key="PurpleRankColor7">#FF290A58</Color>

    <LinearGradientBrush x:Key="PurpleRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource PurpleRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource PurpleRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource PurpleRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource PurpleRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource PurpleRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource PurpleRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource PurpleRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>