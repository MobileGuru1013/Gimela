    <Color x:Key="ControlMouseOverMediumColor">#FFD89F7C</Color>
    <Color x:Key="GlyphColor">#FFD89F7C</Color>
    
    <!--RankColor3-->
    <Color x:Key="ControlMouseOverDarkColor">#FFCB946B</Color>
    <Color x:Key="ControlSelectedBackgroundColor">#FFCB946B</Color>
    <Color x:Key="BackgroundLightColor">#FFCB946B</Color>

    <!--RankColor4-->
    <Color x:Key="ControlLightColor">#FFB48058</Color>
