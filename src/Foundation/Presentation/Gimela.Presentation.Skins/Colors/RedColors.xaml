<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="RedRankColor1">#FFEBA08E</Color>
    <Color x:Key="RedRankColor2">#FFDE8E72</Color>
    <Color x:Key="RedRankColor3">#FFBE5631</Color>
    <Color x:Key="RedRankColor4">#FF962700</Color>
    <Color x:Key="RedRankColor5">#FF7C2305</Color>
    <Color x:Key="RedRankColor6">#FF5C1701</Color>
    <Color x:Key="RedRankColor7">#FF3B0A00</Color>

    <LinearGradientBrush x:Key="RedRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource RedRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource RedRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource RedRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource RedRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource RedRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource RedRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource RedRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>