<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="SkyBlueRankColor1">#FFB6D9E8</Color>
    <Color x:Key="SkyBlueRankColor2">#FF97BFCE</Color>
    <Color x:Key="SkyBlueRankColor3">#FF6D9BB1</Color>
    <Color x:Key="SkyBlueRankColor4">#FF5B869A</Color>
    <Color x:Key="SkyBlueRankColor5">#FF4D7A8D</Color>
    <Color x:Key="SkyBlueRankColor6">#FF395F6F</Color>
    <Color x:Key="SkyBlueRankColor7">#FF1E3F4E</Color>

    <LinearGradientBrush x:Key="SkyBlueRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource SkyBlueRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource SkyBlueRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>