<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="SilverRankColor1">#FFF8FEFF</Color>
    <Color x:Key="SilverRankColor2">#FFDBDEDE</Color>
    <Color x:Key="SilverRankColor3">#FFBFC3C4</Color>
    <Color x:Key="SilverRankColor4">#FF9FA3A3</Color>
    <Color x:Key="SilverRankColor5">#FF7F8383</Color>
    <Color x:Key="SilverRankColor6">#FF5A5C5C</Color>
    <Color x:Key="SilverRankColor7">#FF3D3E3E</Color>

    <LinearGradientBrush x:Key="SilverRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource SilverRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource SilverRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource SilverRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource SilverRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource SilverRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource SilverRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource SilverRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>