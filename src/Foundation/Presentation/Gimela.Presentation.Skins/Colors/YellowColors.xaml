<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="YellowRankColor1">#FFFEE06E</Color>
    <Color x:Key="YellowRankColor2">#FFE2C454</Color>
    <Color x:Key="YellowRankColor3">#FFD1AC26</Color>
    <Color x:Key="YellowRankColor4">#FFB18C05</Color>
    <Color x:Key="YellowRankColor5">#FF907100</Color>
    <Color x:Key="YellowRankColor6">#FF6C5400</Color>
    <Color x:Key="YellowRankColor7">#FF483901</Color>

    <LinearGradientBrush x:Key="YellowRankBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource YellowRankColor1}" Offset="0" />
                <GradientStop Color="{DynamicResource YellowRankColor2}" Offset="0.1" />
                <GradientStop Color="{DynamicResource YellowRankColor3}" Offset="0.3" />
                <GradientStop Color="{DynamicResource YellowRankColor4}" Offset="0.5" />
                <GradientStop Color="{DynamicResource YellowRankColor5}" Offset="0.7" />
                <GradientStop Color="{DynamicResource YellowRankColor6}" Offset="0.9" />
                <GradientStop Color="{DynamicResource YellowRankColor7}" Offset="1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
</ResourceDictionary>